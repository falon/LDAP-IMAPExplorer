<Directory /var/www/html/LDAP-IMAPExplorer>
    Options FollowSymLinks
    AllowOverride None
    AuthBasicProvider authservice
    AuthName "Welcome to LDAP-IMAP Explorer!"
    AuthType basic
    <RequireAny>
            Require all granted
    </RequireAny>
</Directory>
Alias /ldapimap "/var/www/html/LDAP-IMAPExplorer"
